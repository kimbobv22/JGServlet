<?xml version="1.0" encoding="UTF-8"?>
<config>
	<common>
		<debug-level>9</debug-level>
		<character-encoding>UTF-8</character-encoding>
		<dirpath-service>xml/serviceXML/</dirpath-service>
		<dirpath-query>xml/queryXML/</dirpath-query>
		<file-root-path>/Users/kimbobv22/uploads</file-root-path>
		<file-reject-regexp>[\w\-\.]+[\.](php|jsp|asp)</file-reject-regexp>
		<file-image-compression>true</file-image-compression>
		<file-image-compression-regexp>[\w\-\.]+[\.](jpg|jpeg|png)</file-image-compression-regexp>
	</common>
	
	<database name="main" isPrimary="true">
		<jdbc-class>com.mysql.jdbc.Driver</jdbc-class>
		<url>url here</url>
		<user-name>user name</user-name>
		<password>password</password>
		<character-encoding>utf8</character-encoding>
	</database>
	<database name="sub">
		<jdbc-class>com.mysql.jdbc.Driver</jdbc-class>
		<url>url here</url>
		<user-name>user name</user-name>
		<password>password</password>
		<character-encoding>utf8</character-encoding>
	</database>
		
	<custom>
		<option name="customValue">customValue</option>
	</custom>
</config>